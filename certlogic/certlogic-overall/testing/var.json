{
  "name": "use of var",
  "cases": [
    {
      "name": "var-ing non-existing array elements",
      "certLogicExpression": { "var": "1" },
      "assertions": [
        {
          "data": null,
          "expected": null
        },
        {
          "data": [],
          "expected": null
        },
        {
          "data": [1],
          "expected": null
        },
        {
          "data": [1, 2],
          "expected": 2
        }
      ]
    },
    {
      "name": "determine #array <= 1",
      "certLogicExpression": { "!": [ { "var": "1" } ] },
      "assertions": [
        {
          "data": null,
          "expected": true
        },
        {
          "data": [],
          "expected": true
        },
        {
          "data": [null],
          "expected": true
        },
        {
          "data": [null, null],
          "expected": true
        },
        {
          "data": [1],
          "expected": true
        },
        {
          "directive": "skip",
          "data": [1, 2],
          "expected": false
        },
        {
          "directive": "skip",
          "data": [1, 2, 3],
          "expected": false
        }
      ]
    }
  ]
}
